                                        <a href="javascript:ChangePassword()" class="d-flex align-items-center gap-2 dropdown-item">
                                            <i class="ti ti-lock-access fs-6"></i>
                                            <p class="mb-0 fs-3">Đổi mật khẩu</p>
                                        </a>
                                        <hr />
                                        <a href="/Authentication/Logout" class="btn btn-sm btn-outline-dark mx-3 mt-2 d-block">Đăng xuất</a>
                                    </div>
                                </div>
                            </li>
                        </ul>

                    </div>
                </nav>
            </header>
            <div class="container-fluid">
                @if (!SuperbrainManagement.Controllers.CheckUsers.checkcookielogin())
                {
                    Response.Redirect("/Authentication");
                }
                else
                {
                    @RenderBody()
                }
                @{
                    Html.RenderPartial("~/Views/Settings/Datasuggest.cshtml");
                    Html.RenderPartial("~/Views/Settings/_ChangePassword.cshtml");
                }
            </div>
        </div>
    </div>
